<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="testtest.controller.AggregationBind"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<List id="listid">
						<items>
							<CustomListItem>
								<content>
									<Title text="Carrier ID : {Mandt}"></Title>
									<List items="{TOITEM}">
										<items>
											<ObjectListItem title="City From : {Ebelp}" intro="City To :{Matnr}"></ObjectListItem>
										</items>
									</List>
								</content>
							</CustomListItem>
						</items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>