<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="testtest.controller.table">
	<App>
		<pages>
			<Page title="Title">
				<content>
					<Table noDataText="Drop column list items here and columns in the area above" id="__table0" items="{/rows}">
						<items>
							<ColumnListItem id="__item0" >
								<cells>
									<Text text="{field1}" id="__text3"/>
									<Text text="{field2}" id="__text4"/>
									<Text text="{field3}" id="__text5"/></cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column id="__column0">
								<header>
									<Label text="工号" id="__label0"/>
								</header>
							</Column>
							<Column id="__column1">
								<header>
									<Label text="姓名" id="__label1"/>
								</header>
							</Column>
							<Column id="__column2">
								<header>
									<Label text="性别" id="__label2"/>
								</header>
							</Column>
						</columns>
					</Table>
					<List noDataText="Drop list items here" id="__list1" items="{Emp>/EmployeeCollection}">
						<items>
							<StandardListItem type="Navigation" title="{Emp>Empid}" description="{Emp>Empname}" icon="sap-icon://picture" id="__item4" /></items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>